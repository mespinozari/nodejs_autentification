<%- include header %>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >

      <div class="panel panel-info">
        <div class="panel-heading">
          <h3 class="panel-title"><%=data[0].nombre%>, <%=data[0].apellido%></h3>
        </div>
        <div class="panel-body">
          <div class="row">
            <div class="col-md-3 col-lg-3 " align="center"> <img alt="User Pic" src="https://coparmexmetropolitano.mx/themes/websiteV2/uploads/candidatos/alcaldias/marco-calzada.png" class="img-circle img-responsive"> </div>
            <div class=" col-md-9 col-lg-9 "> 
              <table class="table table-user-information">
                <tbody>
                  <tr>
                    <td>Usuario:</td>
                    <td><%=data[0].usuario%></td>
                  </tr>
                  <tr>
                    <td>Celular:</td>
                    <td><%=data[0].mobile%></td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
    </div>
 <%- include footer %>